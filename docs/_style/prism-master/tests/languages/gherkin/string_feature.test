""
"foobar"
"foo<bar>baz"
''
'foobar'
'foo<bar>baz'

----------------------------------------------------

[
	["string", ["\"\""]],
	["string", ["\"foobar\""]],
	["string", [
		"\"foo",
		["outline", "<bar>"],
		"baz\""
	]],
	["string", ["''"]],
	["string", ["'foobar'"]],
	["string", [
        "'foo",
        ["outline", "<bar>"],
        "baz'"
    ]]
