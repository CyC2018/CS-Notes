FOR /l %%a in (5,-1,1) do (TITLE %title% -- closing in %%as)
SET title=%~n0
echo.Hello World
@ECHO OFF
if not defined ProgressFormat set "ProgressFormat=[PPPP]"
EXIT /b
set /a ProgressCnt+=1
IF "%~1" NEQ "" (SET %~1=%new%) ELSE (echo.%new%)

