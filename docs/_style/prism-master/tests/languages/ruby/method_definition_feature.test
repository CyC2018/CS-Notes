class Circle
  def self.of_diameter(diameter)
    new diameter / 2
  end

  def initialize(radius)
    @radius = radius
  end

  def circumference
    Math::PI * radius ** 2
  end

  # Seattle style
  def grow_by factor:
    @radius = @radius * factor
  end
end

